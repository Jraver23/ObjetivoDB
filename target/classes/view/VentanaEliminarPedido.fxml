        <Button fx:id="eliminar" layoutX="25.0" layoutY="129.0" mnemonicParsing="false" onAction="#clicEliminar" text="Eliminar" />
        <Button fx:id="salir" layoutX="167.0" layoutY="129.0" mnemonicParsing="false" onAction="#clicSalir" text="Salir" />
    </children>

</AnchorPane>
